{
    "name": "asseco-voice/laravel-inbox",
    "description": "Laravel handling incoming communication",
    "keywords": [
        "php",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4 || ^8.0",
        "laravel/framework": "^8.0",
        "willdurand/email-reply-parser": "^2.8",
        "zbateson/mail-mime-parser": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4",
        "mockery/mockery": "^1.3.1",
        "fakerphp/faker": "^1.9.1",
        "orchestra/testbench": "^6.0",
        "zendframework/zend-mail": "^2.10"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Asseco\\Inbox\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Asseco\\Inbox\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Asseco\\Inbox\\InboxServiceProvider"
            ]
        }
    }
}
